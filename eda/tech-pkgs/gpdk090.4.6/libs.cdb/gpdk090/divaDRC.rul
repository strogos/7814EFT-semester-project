; procedure for switches !SUGGESTED_CHECK
procedure( switchProc0()
    L68152=geomGetEdge(oxide_in_res inside resdum_sz)
    drc(L68152 width<0.2 "OXIDER.W.1.1: Oxide resistor width must be >= 0.2 um")

    L1707=geomGetEdge(poly_in_res inside resdum_sz)
    drc(L1707 width<0.2 "POLYR.W.1.1: Poly resistor width must be >= 0.2 um")

)

; procedure for switches CHECK_DFM
procedure( switchProc1()
    drc(Poly L11119 enc<0.22
        "NZVT.E.1.DFM: Poly to Oxide enclosure must be >= 0.22 um for DFM")

    drc(L50103 sep<0.14
        "POLY.SP.2.DFM: Poly gate to Poly gate spacing must be >= 0.14 um")
    drc(L50103 notch<0.14
        "POLY.SP.2.DFM: Poly gate to Poly gate spacing must be >= 0.14 um")

    drc(Poly nactive enc<0.20
        "POLY.E.1.DFM: Poly to N+ Oxide enclosure must be >= 0.20 um for DFM")

    drc(Poly pactive enc<0.20
        "POLY.E.2.DFM: Poly to P+ Oxide enclosure must be >= 0.20 um for DFM")

    saveDerived(L20049
        "CONT.SE.1.DFM: Cont to gate spacing must be >= 0.12 um for DFM")
    drc(L99721 L50103 sep<0.12
        "CONT.SE.1.DFM: Cont to gate spacing must be >= 0.12 um for DFM")

    saveDerived(L70108
        "CONT.SE.2.DFM: Cont to 2.5V gate spacing must be >= 0.14 um for DFM")
    drc(L12025 L90375 sep<0.14
        "CONT.SE.2.DFM: Cont to 2.5V gate spacing must be >= 0.14 um for DFM")

    saveDerived(L2239
        "CONT.SE.3.DFM: Poly Cont to Oxide spacing must be >= 0.14 um for DFM")
    drc(cont_poly L49719 sep<0.14
        "CONT.SE.3.DFM: Poly Cont to Oxide spacing must be >= 0.14 um for DFM")

    saveDerived(L66002
        "CONT.SE.4.DFM: Poly Cont to Oxide spacing must be >= 0.16 um for DFM")
    drc(cont_poly L36121 sep<0.16
        "CONT.SE.4.DFM: Poly Cont to Oxide spacing must be >= 0.16 um for DFM")

)

; procedure for switches SUGGESTED_CHECK
procedure( switchProc2()
    L68153=geomGetEdge(oxide_in_res inside resdum_sz)
    drc(L68153 width<1.5 "OXIDER.W.1.2: Oxide resistor width must be >= 1.5 um")

    L22904=geomGetEdge(resdum_sz inside oxide_in_res)
    drc(L22904 width<8.0 "OXIDER.L.1: Oxide resistor length must be >= 8.0 um")

    L1708=geomGetEdge(poly_in_res inside resdum_sz)
    drc(L1708 width<1.5 "POLYR.W.1.2: Poly width must be >= 1.5 um")

    L397=geomGetEdge(resdum_sz inside poly_in_res)
    drc(L397 width<8.0 "POLYR.L.1: Poly resistor length must be >= 8.0 um")

    saveDerived(rule_VIAk_X_3_X_4a
        "VIAk.X.3_VIAk.X.4: Metal1 through Metal6 stack must have two or more stacked Vias at all levels")

    saveDerived(rule_VIAk_X_3_X_4b
        "VIAk.X.3_VIAk.X.4: Metal2 through Metal7 stack must have two or more stacked Vias at all levels")

)

